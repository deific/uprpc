{
  "extends": "../tsconfig.json", // 首先导入 ⑤
  "references": [{ "path": "../uprpc-lib" }], // 因为 client 引用了 common, 故需要将 common 引入进来
  "compilerOptions": {
    "baseUrl": ".",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "dist",
    // "paths": {
    //   "@/*": ["./src/*"]
    //  },
    "removeComments": false,
    "sourceMap": true,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "esnext"
  },
  "exclude": [
    "node_modules",
    "umi",
  ]
}
